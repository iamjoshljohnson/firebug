{
    "name": "ua1-labs/firebug",
    "version": "1.0.0",
    "description": "An expandable PHP debugger panel.",
    "license": "MIT",
    "authors": [
        {
            "name": "UA1 Labs",
            "email": "development@ua1.us"
        }
    ],
    "keywords": [
        "debugging",
        "debugger",
        "var_dump",
        "test",
        "firestudio",
        "fire"
    ],
    "type": "library",
    "require": {},
    "require-dev": {
        "ua1-labs/firetest": "dev-master",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "scripts": {
        "test": "php vendor/ua1-labs/firetest/scripts/runner.php --dir=Test --ext=Test.php",
        "docs": "vendor/bin/phpdoc -d ./Fire -t ./docs/api"
    },
    "autoload": {
        "psr-4": {
            "Fire\\": "Fire/",
            "Test\\": "Test/"
        },
        "files": [
            "functions.php"
        ]
    }

}
